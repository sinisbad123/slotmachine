<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<title>Tagcash Slot Machine</title>

		<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=2.0">

		<link rel="stylesheet" href="css/reset.css" type="text/css" media="screen" />
		<link rel="stylesheet" href="css/style.css" type="text/css" media="screen" />

		<script type="text/javascript" src="//code.jquery.com/jquery-2.1.1.min.js"></script>
		<script type="text/javascript" src="dist/jquery.slotmachine.min.js"></script>

	</head>

	<body>

		<div id="header">
			
			<table cellpadding="0" cellspacing="0">
				<tr>
					<td align="center">
						
						<div class="content">

							<div>TAGCASH SLOT MACHINE</div>
							
							<!-- <div id="textMachine">
								<div>IS AWESOME</div>
								<div>ROCKS!</div>
								<div>IS EASY TO USE</div>
								<div>WORKS WELL</div>
							</div> -->
							<script>
								$(document).ready(function(){
									$("#textMachine").slotMachine({
										active	: 1,
										delay	: 450,
										auto	: 1500
									});
								});
							</script>

						</div>
					</td>
				</tr>
			</table>
			
		</div>
		
		<div class="line" style="padding-top:10px;">
			
			<div class="content" style="text-align: center; height: 500px;">
				
				<div style="clear:both">
					<p class="title" style="margin-left: -25px;"></p>
				</div>
				
				<div style="clear:both; margin-top: 80px;">
					<div id="machine1" class="slotMachine" style="margin-left: -65px;">
						<div class="slot slot1"></div>
						<div class="slot slot2"></div>
						<div class="slot slot3"></div>
						<div class="slot slot4"></div>
						<!-- <div class="slot slot5"></div>
						<div class="slot slot6"></div> -->
					</div>
					
					<div id="machine2" class="slotMachine">
						<div class="slot slot1"></div>
						<div class="slot slot2"></div>
						<div class="slot slot3"></div>
						<div class="slot slot4"></div>
						<!-- <div class="slot slot5"></div>
						<div class="slot slot6"></div> -->
					</div>
					
					<div id="machine3" class="slotMachine">
						<div class="slot slot1"></div>
						<div class="slot slot2"></div>
						<div class="slot slot3"></div>
						<div class="slot slot4"></div>
						<!-- <div class="slot slot5"></div>
						<div class="slot slot6"></div> -->
					</div>
					<script>
						$(document).ready(function(){
							$(".audioDemo").trigger('load');
							$(".audioDemo").trigger('play');
							var machine1 = $("#machine1").slotMachine({
								active	: 0,
								delay	: 500
							});
							
							var machine2 = $("#machine2").slotMachine({
								active	: 1,
								delay	: 500
							});
							
							window.machine3 = $("#machine3").slotMachine({
								active	: 2,
								delay	: 500
							});
							$("#slotMachineButtonShuffle").click(function(){
								$('.title').empty();
								machine1.shuffle();
								machine2.shuffle();
								machine3.shuffle();
							});
							$("#slotMachineButtonStop").click(function(){
								if( machine1.isRunning()){
									machine1.stop(1);
									setTimeout(function(){
										machine2.stop(1);
									}, 500);
									setTimeout(function(){
										machine3.stop(1);
									}, 1000);
									setTimeout(function(){
									$count = 0;
									$('.check').trigger('click');
									$count = 1;
									setTimeout(function(){
											$count = 2;
											$('.check').trigger('click');
										},1400);
									},1200);
								}
								
							});
							$('.check').click(function(){
								if($count == 2){
									getIndexes();
								}
							});
							function getIndexes(){
								index1 = machine1.active().index;
								index2 = machine2.active().index;
								index3 = machine3.active().index;

								if((index1 == index2) && (index2 == index3)){
									if(index1 == 0){
										$('.title').empty();
										$('.title').text('YOU WIN!');
									}else{
										$('.title').empty();
										$('.title').text('YOU MUST GET THE TAG LOGOS. TRY AGAIN!');
									}
								}else{
									if(((index1 == index2) && index1 == 0) || ((index2 == index3) && index2 == 0) || ((index1 == index3) && index1 == 0)){
										$('.title').empty();
										$('.title').text('ALMOST HAD IT!');
									}else{
										$('.title').empty();
										$('.title').text('BETTER LUCK NEXT TIME!');
									}
								}
							}
						});
					</script>
				</div>
				
			</div>
			
			<div class="content" style="padding-left:500px; margin-top:-100px">
				<div id="slotMachineButtonShuffle" class="slotMachineButton" style="font-size: 25px">Shuffle!</div>
				<div id="slotMachineButtonStop" class="slotMachineButton" style="font-size: 25px">Stop!</div>
				<input type="hidden" class="check">
			</div>
			
		</div>
		
		
		

	</body>
</html>
